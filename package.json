{
  "name": "puppeteer-api-examples",
  "version": "1.0.0",
  "description": "An example of ES6 page objects using the new puppeteer framework from google chrome",
  "main": "index.js",
  "scripts": {
    "test": "mocha --timeout 999999 lib/index.js",
    "lint": "tslint --fix src/**/*.ts",
    "moveJsonFiles": "copyfiles -u 1 src/selectors/* src/config/* lib",
    "build": "npm run lint && rimraf lib && npm run moveJsonFiles && tsc --outDir lib"
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdavis61/puppeteer-api-examples.git"
  },
  "author": "James Davis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jdavis61/puppeteer-api-examples/issues"
  },
  "homepage": "https://github.com/jdavis61/puppeteer-api-examples#readme",
  "dependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.5",
    "puppeteer": "latest"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.32",
    "@types/node": "^10.5.3",
    "@types/puppeteer": "^1.5.1",
    "copyfiles": "^2.0.0",
    "pre-commit": "latest",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.7.0",
    "typescript": "^2.9.2"
  }
}
