{
  "name": "puppeteer-api-examples",
  "version": "1.0.0",
  "description": "An example of ES6 page objects using the new puppeteer framework from google chrome",
  "main": "index.js",
  "scripts": {
    "test": "mocha --timeout 999999 lib/index.js",
    "lint": "eslint --fix src",
    "moveJsonFiles": "copyfiles -u 1 src/selectors/* src/config/* lib",
    "build": "rimraf lib && babel src --out-dir lib && npm run moveJsonFiles"
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdavis61/puppeteer-api-examples.git"
  },
  "author": "James Davis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jdavis61/puppeteer-api-examples/issues"
  },
  "homepage": "https://github.com/jdavis61/puppeteer-api-examples#readme",
  "dependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.5",
    "puppeteer": "latest"
  },
  "devDependencies": {
    "babel-cli": "latest",
    "babel-plugin-transform-runtime": "latest",
    "babel-preset-env": "latest",
    "copyfiles": "^2.0.0",
    "eslint": "latest",
    "eslint-config-airbnb-base": "latest",
    "eslint-plugin-import": "latest",
    "pre-commit": "latest",
    "rimraf": "^2.6.2"
  },
  "babel": {
    "presets": [
      "stage-3",
      "latest"
    ]
  }
}
